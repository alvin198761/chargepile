<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>充电站基本信息</title>
    <meta http-equiv="pragma" content="no-cache" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <link rel="stylesheet" type="text/css" href="../../Scripts/jquery-easyui-1.3.1/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../Scripts/jquery-easyui-1.3.1/themes/icon.css" />
    <script src="../../Scripts/jquery-easyui-1.3.1/jquery-1.8.0.min.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-easyui-1.3.1/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-easyui-1.3.1/locale/easyui-lang-zh_CN.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-easyui-1.3.1/easyui-validate.js" type="text/javascript"></script>
    <script src="../../Scripts/ChargStation/StationInfo.js" type="text/javascript"></script>
    <style type="text/css">
    .textright
        {
            width: 320px;
            text-align: right;
        }
         .textleft
        {
            text-align: left;
        }
        *
        {
            margin:0;
            padding:0;
            font-size: 12px;
        }
    </style>
</head>
<body class="easyui-layout">
    <div data-options="region:'center'">
        <!--新建充电桩基本信息-->
        <div id="stationinfo" style="width: 790px; margin: 0 auto; height: 430px; text-align:center;">
            <div style=" height:45px; line-height: 45px; margin-top: 3px;">
                <label id="title" style="font-size:35px; font-family: FZXiaoBiaoSong-B05S,黑体; color: RGB(0,113,141); font-weight: bold;">
                </label>
            </div>
            <hr style=" color:RGB(0,113,141);">
            <table cellpadding="4" cellspacing="1" width="100%" style="border: 0; overflow: hidden; margin-top:5px;"
                class="detailtable">
                <tr style="height: 28px">
                    <td class="textright">
                        充电站名称：
                    </td>
                    <td class="textleft">
                        <input readonly="readonly" type="text" id="zhanmc" name="zhanmc" style="width: 200px; height:20px; background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px" id="Tr1">
                    <td class="textright">
                        充电站简称：
                    </td>
                    <td class="textleft">
                        <input readonly="readonly" type="text" id="zhanjc" name="zhanjc" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        详细地址：
                    </td>
                    <td class="textleft">
                        <input readonly="readonly" type="text" id="xxdz" name="xxdz" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        经度：
                    </td>
                    <td  class="textleft">
                        <input readonly="readonly" type="text" id="jd" name="jd" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        纬度：
                    </td>
                    <td  class="textleft">
                        <input readonly="readonly" type="text" id="wd" name="wd" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        场地业主单位：
                    </td>
                    <td  class="textleft">
                        <input readonly="readonly" type="text" id="yzdw" name="yzdw" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        场地业主联系人：
                    </td>
                    <td  class="textleft">
                        <input readonly="readonly" type="text" id="lxr" name="lxr" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        场地业主联系电话：
                    </td>
                    <td  class="textleft">
                        <input readonly="readonly" type="text" id="lxdh" name="lxdh" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        建站时间：
                    </td>
                    <td  class="textleft">
                        <input readonly="readonly" type="text" id="jzsj" name="jzsj" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
                <tr style="height: 28px">
                    <td class="textright">
                        投运时间：
                    </td>
                    <td class="textleft">
                        <input readonly="readonly" type="text" id="tysj" name="tysj" style="width: 200px;height:20px;background-color:RGB(0,113,141);color:#fff;" class="easyui-validatebox"/>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>
