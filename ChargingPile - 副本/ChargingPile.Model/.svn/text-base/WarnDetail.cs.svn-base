using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ChargingPile.Model
{
    public class WarnDetail
    {
        /// <summary>
        /// 唯一Id
        /// </summary>
        public string Id { get; set; }

        /// <summary>
        /// 告警方式Id
        /// </summary>
        public string WarnId { get; set; }

        /// <summary>
        /// 采集数据Id
        /// </summary>
        public string GatherId { get; set; }

        /// <summary>
        /// 目标地址
        /// </summary>
        public string Address { get; set; }

        /// <summary>
        /// 发送成功
        /// </summary>
        public int? IsSuccess { get; set; }

        /// <summary>
        /// 发送时间
        /// </summary>
        public DateTime? SendDT { get; set; }

        /// <summary>
        /// 告警内容
        /// </summary>
        public string WarnContext { get; set; }

        /// <summary>
        /// 告警方式
        /// </summary>
        public string WarnType { get; set; }

        /// <summary>
        /// 处理标志
        /// </summary>
        public int? ProcessFlag { get; set; }

        /// <summary>
        /// 更新时间
        /// </summary>
        public DateTime? UpdateDT { get; set; }

        /// <summary>
        /// 创建时间
        /// </summary>
        public DateTime? CreateDT { get; set; }

        public override string ToString()
        {
            return "Id:" + this.Id + 
                ",WarnId:" + this.WarnId +
                ",GatherId:" + this.GatherId +
                ",Address:" + this.Address +
                ",IsSuccess:" + this.IsSuccess +
                ",SendDT:" + this.SendDT +
                ",WarnContext:" + this.WarnContext +
                ",WarnType:" + this.WarnType +
                ",ProcessFlag:" + this.ProcessFlag +
                ",CreateDT:" + this.CreateDT +
                ",UpdateDT:" + this.UpdateDT;
        }
    }
}
